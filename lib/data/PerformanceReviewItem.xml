<entity entity-name="PerformanceReviewItem" package-name="mantle.humanres.ability">
        <field name="performanceReviewId" type="id" is-pk="true"/>
        <field name="performanceReviewItemSeqId" type="id" is-pk="true"/>
        <field name="reviewItemTypeEnumId" type="id"/>
        <field name="reviewRatingEnumId" type="id"/>
        <field name="comments" type="text-medium"/>
        <relationship type="one" related-entity-name="PerformanceReview"/>
        <relationship type="one" title="PerformanceReviewItemType" related-entity-name="moqui.basic.Enumeration">
            <key-map field-name="reviewItemTypeEnumId"/>
        </relationship>
        <relationship type="one" title="PerformanceReviewRating" related-entity-name="moqui.basic.Enumeration">
            <key-map field-name="reviewRatingEnumId"/>
        </relationship>
        <seed-data>
            <!-- Performance Review Item Type -->
            <moqui.basic.EnumerationType description="Performance Review Item Type" enumTypeId="PerformanceReviewItemType"/>
            <moqui.basic.Enumeration enumId="PRIT_TECH" description="Technical skills" enumTypeId="PerformanceReviewItemType"/>
            <moqui.basic.Enumeration enumId="PRIT_RESP" description="Responsibility" enumTypeId="PerformanceReviewItemType"/>
            <moqui.basic.Enumeration enumId="PRIT_ATT" description="Attitude" enumTypeId="PerformanceReviewItemType"/>
            <moqui.basic.Enumeration enumId="PRIT_COMM" description="Communication skills" enumTypeId="PerformanceReviewItemType"/>
            <moqui.basic.Enumeration enumId="PRIT_JOBSAT" description="Job Satisfaction" enumTypeId="PerformanceReviewItemType"/>

            <!-- Performance Review Rating -->
            <moqui.basic.EnumerationType description="Performance Review Rating" enumTypeId="PerformanceReviewRating"/>
            <moqui.basic.Enumeration enumId="PRR_1" sequenceNum="1" description="Fails to Meet Expectations" enumTypeId="PerformanceReviewRating"/>
            <moqui.basic.Enumeration enumId="PRR_2" sequenceNum="2" description="Inconsistently Fulfills" enumTypeId="PerformanceReviewRating"/>
            <moqui.basic.Enumeration enumId="PRR_3" sequenceNum="3" description="Fulfilled Expectations" enumTypeId="PerformanceReviewRating"/>
            <moqui.basic.Enumeration enumId="PRR_4" sequenceNum="4" description="Frequently Exceeds" enumTypeId="PerformanceReviewRating"/>
            <moqui.basic.Enumeration enumId="PRR_5" sequenceNum="5" description="Consistently Exceeds" enumTypeId="PerformanceReviewRating"/>
        </seed-data>
    </entity>