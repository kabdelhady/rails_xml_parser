<entity entity-name="TrainingClass" package-name="mantle.humanres.trainings">
        <field name="trainingClassId" type="id" is-pk="true"/>
        <field name="trainingClassTypeEnumId" type="id"/>
        <field name="description" type="text-medium"/>
        <relationship type="one" title="TrainingClassType" related-entity-name="moqui.basic.Enumeration">
            <key-map field-name="trainingClassTypeEnumId"/>
        </relationship>
        <seed-data>
            <!-- Training Class Type -->
            <moqui.basic.EnumerationType description="Training Class Type" enumTypeId="TrainingClassType"/>
            <moqui.basic.Enumeration enumId="BASIC_TRAINING" description="Basic Training" enumTypeId="TrainingClassType"/>
            <moqui.basic.Enumeration enumId="ONJOB_TRAINING" description="On Job Training" enumTypeId="TrainingClassType"/>
            <moqui.basic.Enumeration enumId="ADVAN_TRAINING" description="Advance Training" enumTypeId="TrainingClassType"/>
            <moqui.basic.Enumeration enumId="IMPRO_TRAINING" description="Improvement Training" enumTypeId="TrainingClassType"/>
        </seed-data>
    </entity>